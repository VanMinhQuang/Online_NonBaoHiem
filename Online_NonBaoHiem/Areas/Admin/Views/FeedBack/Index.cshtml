@model IEnumerable<Online_NonBaoHiem.Models.Feedback>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            Mã FeedBack
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdBill)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GhiChu)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdFeedback)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdBill)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GhiChu)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.IdFeedback }) |
